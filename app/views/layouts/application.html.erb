<!DOCTYPE html>
<html>
  <head>
    <title>RailsWikinimous</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag "https://use.fontawesome.com/releases/v5.8.1/css/all.css" %>

    <%= stylesheet_link_tag  "https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar bg-light nav-custom-font">
      <!-- <a class="navbar-brand" href="#">AggyWikiNimous</a> -->
      <%= link_to 'AggyWikiNimous', articles_path, class: "navbar-brand" %>
      <%= link_to 'Write new article', new_article_path, class: "navbar-brand" %>
    </nav>
    <%= yield %>
  </body>
</html>
